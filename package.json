{
    "name": "opensslutils",
    "displayName": "opensslutils",
    "description": "",
    "version": "0.0.1",
    "publisher": "ffaraone",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showOpenSSLPreview",
        "onCommand:extension.convertCrtToPem",
        "onCommand:extension.convertPemToCrt",
        "onCommand:extension.generateKeyCsr"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showOpenSSLPreview",
                "title": "Show OpenSSL preview",
                "category": "OpenSSL"
            },
            {
                "command": "extension.generateKeyCsr",
                "title": "Generate Csr",
                "category": "OpenSSL"
            },
            {
                "command": "extension.convertCrtToPem",
                "title": "OpenSSL: Crt to Pem"
            },
            {
                "command": "extension.convertPemToCrt",
                "title": "OpenSSL: Pem to Crt"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "extension.convertCrtToPem",
                    "when": "1 == 2"
                },
                {
                    "command": "extension.convertPemToCrt",
                    "when": "1 == 2"
                }
            ],
            "explorer/context": [
                {
                    "when": "!explorerResourceIsFolder",
                    "command": "extension.convertCrtToPem",
                    "group": "z_openssl@1"
                },
                {
                    "when": "!explorerResourceIsFolder",
                    "command": "extension.convertPemToCrt",
                    "group": "z_openssl@2"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
